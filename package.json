  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "verovio": "^5.2.0",
    "vite": "^6.2.4",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "verovio": "^3.15.0 || ^4.0.0 || ^5.0.0",
