  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "prettier": "^3.0.3",
    "verovio": "^4.0.0",
    "vite": "^5.0.2",
    "vue": "^3.2.39"
  },
  "peerDependencies": {
    "verovio": "^3.15.0 || ^4.0.0",
    "vue": "^3.2.39"
